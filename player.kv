#:import entities entities

<Player>:
    size_hint: None, None
    size: 35, 35

    canvas:
        Color:
            rgba: .6, .6, .6, 1
        Rectangle:
            pos: self.pos
            size: self.size

<Ground>:
    canvas:
        Color:
            rgba: .35, .2, 0, 1
        Rectangle:
            pos: self.x - 1000, self.y - 1000
            size: 2000, 2000
